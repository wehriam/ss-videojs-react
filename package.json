{
  "name": "ss-videojs-react",
  "version": "0.1.20",
  "description": "React Component for video.js",
  "main": "dist/video.js",
  "scripts": {
    "build": "babel -d dist/ src/",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goskipstone/videojs-react.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "keywords": [
    "video.js",
    "react"
  ],
  "author": "mattotodd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattotodd/videojs-react/issues"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ],
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "homepage": "https://github.com/mattotodd/videojs-react#readme",
  "peerDependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "lodash": "^4.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.0",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19"
